msgid "``conjure``"
msgstr ""

#: ../../../../Modules/FindImageMagick.cmake:30
msgid "``convert``"
msgstr ""

#: ../../../../Modules/FindImageMagick.cmake:31
msgid "``display``"
msgstr ""

#: ../../../../Modules/FindImageMagick.cmake:32
msgid "``identify``"
msgstr ""

#: ../../../../Modules/FindImageMagick.cmake:33
msgid "``import``"
msgstr ""

#: ../../../../Modules/FindImageMagick.cmake:34
msgid "``mogrify``"
msgstr ""

#: ../../../../Modules/FindImageMagick.cmake:35
msgid "``montage``"
msgstr ""

#: ../../../../Modules/FindImageMagick.cmake:36
msgid "``stream``"
msgstr ""

